										<xs:documentation>Подтверждение создания денежного перевода.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="send-request" type="transfer-legal-send">
									<xs:annotation>
										<xs:documentation>Отправка денежного перевода.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="send-reply" type="transfer-confirm">
									<xs:annotation>
