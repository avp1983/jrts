<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2013 (http://www.altova.com) by  () -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="xmldsig-core-schema.xsd"/>
	<xs:complexType name="directory-request">
		<xs:annotation>
			<xs:documentation>Запрос справочника.</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0">
			<xs:element name="filter" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Фильтр справочника.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="field" type="text25-type" use="required">
						<xs:annotation>
							<xs:documentation>Поле, по которому накладывается фильтр.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="value" type="text255-type" use="required">
						<xs:annotation>
							<xs:documentation>Значение.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="type" use="required">
			<xs:annotation>
				<xs:documentation>Тип справочника.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text25-type">
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="change-date" type="datetime-type">
			<xs:annotation>
				<xs:documentation>Дата изменения</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="chunk-number" type="integer-type">
			<xs:annotation>
				<xs:documentation>Номер фрагмента справочника</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="directory-reply">
		<xs:annotation>
			<xs:documentation>Справочник.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="header">
				<xs:annotation>
					<xs:documentation>Секция заголовков.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="column" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Название столбца.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="col" type="integer-type" use="required">
									<xs:annotation>
										<xs:documentation>Номер столбца.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="value" use="required">
									<xs:annotation>
										<xs:documentation>Значение.</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="text255-type">
											<xs:minLength value="1"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="data" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Секция данных.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="row" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Строка справочника.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence minOccurs="0" maxOccurs="unbounded">
									<xs:element name="column" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:attribute name="col" type="integer-type">
												<xs:annotation>
													<xs:documentation>Номер столбца.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="value" type="xs:string">
												<xs:annotation>
													<xs:documentation>Значение.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="type" type="text20-type" use="required">
			<xs:annotation>
				<xs:documentation>Название справочника</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="change-date" type="datetime-type" use="required">
			<xs:annotation>
				<xs:documentation>Дата изменения</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="chunk-number" type="integer-type" use="required">
			<xs:annotation>
				<xs:documentation>Номер фрагмента справочника</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="last-chunk" type="int01-type" use="required">
			<xs:annotation>
				<xs:documentation>1 - последний фрагмент справочника, 0 - необходимо отправить дополнительный запрос на получение следующего фрагмента справочника.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="error">
		<xs:annotation>
			<xs:documentation>Ошибка из банка.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Id" use="optional" fixed="DocData">
			<xs:annotation>
				<xs:documentation>Идентификатор подписываемого элемента</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="code" type="integer-type" use="required">
			<xs:annotation>
				<xs:documentation>Код ошибки.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="text" use="required">
			<xs:annotation>
				<xs:documentation>Текст ошибки.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text255-type">
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="letter">
		<xs:annotation>
			<xs:documentation>Предок всех писем.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="text" type="xs:string">
				<xs:annotation>
					<xs:documentation>Текст письма.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="date" type="datetime-type" use="required">
			<xs:annotation>
				<xs:documentation>Дата письма.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="subject" use="required">
			<xs:annotation>
				<xs:documentation>Тема письма.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text255-type">
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="type" type="integer-type">
			<xs:annotation>
				<xs:documentation>Код типа отправляемого письма (из справочника типов отправляемых писем).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="letter-answer">
		<xs:annotation>
			<xs:documentation>Ответ на письмо клиента в банк.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="letter">
				<xs:attribute name="number" type="integer-type">
					<xs:annotation>
						<xs:documentation>Номер письма.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="status" type="text20-type" use="required">
					<xs:annotation>
						<xs:documentation>Статус документа: 
принят, исполнен, отказан.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="client-official" type="text40-type" use="required">
					<xs:annotation>
						<xs:documentation>ФИО уполномоченного лица отправителя сообщения.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="bank-official" type="text50-type">
					<xs:annotation>
						<xs:documentation>ФИО уполномоченного лица банка.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="note" type="text255-type">
					<xs:annotation>
						<xs:documentation>Комментарий банка.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="letter-client">
		<xs:annotation>
			<xs:documentation>Письмо клиента в банк.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="letter">
				<xs:attribute name="number" type="integer-type">
					<xs:annotation>
						<xs:documentation>Номер письма.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="client-official" use="required">
					<xs:annotation>
						<xs:documentation>ФИО уполномоченного лица отправителя сообщения.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="text40-type">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="letter-bank">
		<xs:annotation>
			<xs:documentation>Письмо банка клиенту.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="letter">
				<xs:attribute name="number" type="integer-type" use="required">
					<xs:annotation>
						<xs:documentation>Номер письма.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="receiver-id" type="integer-type" use="required">
					<xs:annotation>
						<xs:documentation>Идентификатор пункта-получателя сообщения.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="contact-type">
		<xs:annotation>
			<xs:documentation>Контактные данные пункта</xs:documentation>
		</xs:annotation>
		<xs:attribute name="phone">
			<xs:annotation>
				<xs:documentation>Контактный телефон.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text40-type">
					<xs:minLength value="1" fixed="true"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="e-mail" type="text50-type">
			<xs:annotation>
				<xs:documentation>Контактный e-mail.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="address-type">
		<xs:annotation>
			<xs:documentation>Адрес</xs:documentation>
		</xs:annotation>
		<xs:attribute name="zip">
			<xs:annotation>
				<xs:documentation>Индекс.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="place-id" type="integer-type">
			<xs:annotation>
				<xs:documentation>Идентификатор населенного пункта из справочника населённых пунктов.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="street-type">
			<xs:annotation>
				<xs:documentation>Идентификатор типа улицы из справочника типов улиц.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="street">
			<xs:annotation>
				<xs:documentation>Улица.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text250-type"/>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="house">
			<xs:annotation>
				<xs:documentation>Дом.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text250-type"/>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="flat">
			<xs:annotation>
				<xs:documentation>Квартира.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text250-type"/>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="location-type">
		<xs:annotation>
			<xs:documentation>Адреса пункта</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0">
			<xs:element name="low-address" type="address-type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Юридический адрес.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="post-address" type="address-type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Почтовый адрес.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="country-numeric" use="required">
			<xs:annotation>
				<xs:documentation>Числовой код ISO страны ад-реса, например, 643.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="numeric3-type">
					<xs:minLength value="1" fixed="true"/>
					<xs:maxLength value="3" fixed="true"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="works">
		<xs:annotation>
			<xs:documentation>Расписание работы.</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="data" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Секция данных</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="day-start">
						<xs:annotation>
							<xs:documentation>День недели начала расписания.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="integer-type">
								<xs:pattern value="[1-7]{1}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="day-end">
						<xs:annotation>
							<xs:documentation>День недели окончания расписания.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="integer-type">
								<xs:pattern value="[1-7]{1}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="worktime-start" type="time-type">
						<xs:annotation>
							<xs:documentation>Время начала рабочего дня.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="worktime-end" type="time-type">
						<xs:annotation>
							<xs:documentation>Время окончания рабочего дня.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="breaktime-start" type="time-type">
						<xs:annotation>
							<xs:documentation>Начало перерыва.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="breaktime-end" type="time-type">
						<xs:annotation>
							<xs:documentation>Конец перерыва.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="point">
		<xs:annotation>
			<xs:documentation>Данные пункта отправки / выдачи переводов.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="location" type="location-type">
				<xs:annotation>
					<xs:documentation>Информация о местонахождении пункта.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contact" type="contact-type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о контактном лице.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="works" type="works" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Расписание работы.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="point-id" type="integer-type">
			<xs:annotation>
				<xs:documentation>Уникальный идентификатор. Отсутствует при добавлении пункта.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="link-point" type="integer-type" use="required">
			<xs:annotation>
				<xs:documentation>Идентификатор вышестоящего пункта</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="point-name-rus" use="required">
			<xs:annotation>
				<xs:documentation>Наименование на русском языке.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text255-type">
					<xs:minLength value="1" fixed="true"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="point-name-int" use="required">
			<xs:annotation>
				<xs:documentation>Наименование на английском языке.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text255-type">
					<xs:minLength value="1" fixed="true"/>
					<xs:pattern value="\P{IsCyrillic}*"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="enabled" type="integer-type">
			<xs:annotation>
				<xs:documentation>Признак работающего пункта.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="enableddate" type="datetime-type">
			<xs:annotation>
				<xs:documentation>Дата ввода в действие пункта.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="point-id-ext" type="text150-type">
			<xs:annotation>
				<xs:documentation>Идентификатор пункта во внешней системе</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="point-confirm">
		<xs:annotation>
			<xs:documentation>Подтверждение выполнения операции над пунктом.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="point-id" type="integer-type" use="required">
			<xs:annotation>
				<xs:documentation>Уникальный идентификатор.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="point-id-ext">
			<xs:annotation>
				<xs:documentation>Идентификатор пункта во внешней системе</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="result-code" type="xs:integer">
			<xs:annotation>
				<xs:documentation>Результат выполнения операции</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="error-text">
			<xs:annotation>
				<xs:documentation>Текст ошибки.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text255-type">
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="id-type">
		<xs:annotation>
			<xs:documentation>Идентификатор запроса в очереди на обработку.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="date-create" type="integer-type" use="required">
			<xs:annotation>
				<xs:documentation>Дата добавления запроса  в очередь (целое число)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="time-create" type="integer-type" use="required">
			<xs:annotation>
				<xs:documentation>Время добавления запроса  в очередь (целое число)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="report-request">
		<xs:annotation>
			<xs:documentation>Запрос отчета.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="type" use="required">
			<xs:annotation>
				<xs:documentation>Тип отчета.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="text25-type">
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="date-from" type="datetime-type" use="required">
			<xs:annotation>
				<xs:documentation>Дата начала периода.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="date-to" type="datetime-type" use="required">
			<xs:annotation>
				<xs:documentation>Дата окончания периода.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="currency-alpha" type="alpha3-type">
			<xs:annotation>
				<xs:documentation>Код валюты отчета.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="report-reply">
		<xs:annotation>
			<xs:documentation>Отчет.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="id-type">
				<xs:sequence>
					<xs:element name="header">
						<xs:annotation>
							<xs:documentation>Секция заголовков.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="column" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Название столбца.</xs:documentation>
									</xs:annotation>
									<xs:complexType>
