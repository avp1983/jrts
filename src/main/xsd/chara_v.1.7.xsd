							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="integer-type" use="required">
									<xs:annotation>
										<xs:documentation>Идентификатор категории</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="params">
				<xs:annotation>
					<xs:documentation>Список параметров передаваемых при платеже</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence maxOccurs="unbounded">
						<xs:element name="param" type="param-type">
							<xs:annotation>
								<xs:documentation>Описание параметра</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
		<xs:attribute name="id" type="provider-id-type" use="required">
			<xs:annotation>
				<xs:documentation>Идентификатор провайдера</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="text255-type" use="required">
